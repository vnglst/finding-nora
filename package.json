{
  "name": "finding-nora",
  "version": "3.6.0",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "test": "react-scripts test --verbose=false",
    "eject": "react-scripts eject",
    "verify": "yarn test; yarn build; yarn cypress:ci",
    "deploy": "yarn build && netlify deploy ./build",
    "cypress:ci": "cypress run",
    "cypress:open": "cypress open",
    "coveralls": "yarn test --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.8",
    "@types/howler": "^2.1.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "coveralls": "^3.0.9",
    "cypress": "^3.8.1",
    "global": "^4.4.0",
    "react-scripts": "^3.3.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@bugsnag/js": "^6.5.0",
    "@bugsnag/plugin-react": "^6.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "classnames": "^2.2.6",
    "howler": "^2.1.3",
    "minimal-analytics": "^0.1.15",
    "prevent-double-tap-zoom": "^2.0.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
