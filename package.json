{
  "name": "finding-nora",
  "version": "3.2.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "pretest": "yarn lint",
    "test": "react-scripts-ts test --env=jsdom",
    "test:ci": "yarn test; yarn build; yarn storybook:build; yarn cypress:ci",
    "eject": "react-scripts-ts eject",
    "deploy": "yarn build && netlify deploy ./build",
    "deploy:now": "yarn build && now alias $(now ./build --public --static) nora.now.sh",
    "cypress:ci": "cypress run --record --key d9fcfab5-00ab-4c62-900a-80aefc8e3117",
    "cypress:open": "cypress open",
    "lint": "tslint --project ./",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@reach/router": "^1.2.0",
    "classnames": "^2.2.6",
    "howler": "^2.0.15",
    "prevent-double-tap-zoom": "^2.0.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@types/howler": "^2.0.5",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.10.3",
    "@types/reach__router": "^1.0.1",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.9",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cypress": "^3.1.0",
    "http-server": "^0.11.1",
    "react-scripts-ts": "2.17.0",
    "source-map-explorer": "^1.6.0",
    "typescript": "^3.0.3"
  }
}
